        <class_name>Processor</class_name>
        <class_source>import java.text.SimpleDateFormat;
import java.text.ParseException;
import java.util.Date;

public static long day = 86400000;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	Object[] r = getRow(); 

	String node_time_tmp = getVariable("_node_time");

	if (r == null)
	{
		setOutputDone();
		return false;
	}

	if (first)
	{
		first = false;
		
		logBasic(node_time_tmp);
	}

	r = createOutputRow(r, data.outputRowMeta.size());

	String[] comment_time_list = get(Fields.In,"_comment_time_groupby").getString(r).split("_");


	long _3_days_comment_cnt = 0;
	long _7_days_comment_cnt = 0;
	long _15_days_comment_cnt = 0;
	long _30_days_comment_cnt = 0;

	Long node_time = dateToStamp(node_time_tmp);

	for(int i = 0;i &lt; comment_time_list.length;i++)
	{
		Long tmp_time = Long.valueOf(comment_time_list[i]);

		if (tmp_time >= node_time - (day * 30) &amp;&amp; tmp_time &lt;= node_time)
		{
			if (tmp_time >= (node_time - day * 15))
			{
				if (tmp_time >= (node_time - day * 7))
				{
					if (tmp_time >= (node_time - day * 3))
					{
						_3_days_comment_cnt++;
					}
					_7_days_comment_cnt++;
				}
				_15_days_comment_cnt++;
			}
			_30_days_comment_cnt++;
		}		
	}

	get(Fields.Out,"_3_days_comment_cnt").setValue(r,_3_days_comment_cnt);
	get(Fields.Out,"_7_days_comment_cnt").setValue(r,_7_days_comment_cnt);
	get(Fields.Out,"_15_days_comment_cnt").setValue(r,_15_days_comment_cnt);
	get(Fields.Out,"_30_days_comment_cnt").setValue(r,_30_days_comment_cnt);

	Long date = Long.valueOf(getVariable("DATE"));
	get(Fields.Out, "_date").setValue(r,date);

	putRow(data.outputRowMeta,r);


	return true; 
}


public static Long dateToStamp(String s)
{
	SimpleDateFormat simpleDateFormat = new SimpleDateFormat("yyyy-MM-dd HH:mm:ss");
	Date date = new Date();
	try
	{
		date = simpleDateFormat.parse(s);
	}
	catch (ParseException e)
	{	
	}
	Long ts = date.getTime();
	return ts;
}

</class_source>
