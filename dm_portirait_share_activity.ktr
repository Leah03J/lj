        <class_source>import java.text.SimpleDateFormat;
import java.text.ParseException;
import java.util.Date;

import java.lang.Math;
public static int cnt;
public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	Object[] r = getRow(); 
	if (r == null)
	{
		setOutputDone();
		return false;
	}


	
	if (first)
	{
		first = false;
		
	}
		
	
	r = createOutputRow(r, data.outputRowMeta.size()); 

	Long id = get(Fields.In,"_id").getInteger(r);
	Long code_id = get(Fields.In,"_code_id").getInteger(r);

	
	if (code_id == null)
	{
		get(Fields.Out,"_code_id").setValue(r,id);
	
		Long date = Long.valueOf(getVariable("DATE"));
		get(Fields.Out, "_date").setValue(r,date);
	
		putRow(data.outputRowMeta,r);		
	}
	else
	{
		putRow(data.outputRowMeta,r);
	}

	
	return true; 
}


